  "version": "0.2.0",
  "configurations": [

  {
    "type": "node",
    "request": "attach",
    "name": "Attach",
    "port": 9229,
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to test suite",
      "skipFiles": [
        "<node_internals>/**"
      ],
    },

    {
			"type": "node",
			"request": "launch",
			"name": "Jest: Client Test",
			"program": "${workspaceFolder}/node_modules/.bin/jest",
			"env": {
				"NODE_ENV": "test"
			},
			"args": [
				"--runInBand",
				"tests/lib/index.test.js"
			],
			"skipFiles": [ "<node_internals>/**" ],
		},


  ]
}